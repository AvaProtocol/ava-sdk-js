{
  "name": "@avaprotocol/root",
  "version": "1.2.1-dev.9",
  "engines": {
    "node": ">=20.18.0",
    "yarn": ">=1.22.19"
  },
  "private": true,
  "type": "commonjs",
  "license": "Apache-2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "version": "changeset version",
    "proto-download": "curl -s https://raw.githubusercontent.com/AvaProtocol/EigenLayer-AVS/staging/protobuf/avs.proto > grpc_codegen/avs.proto",
    "protoc-gen": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./grpc_codegen/ --grpc_out=grpc_js:./grpc_codegen/ --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./grpc_codegen --proto_path=./grpc_codegen grpc_codegen/avs.proto",
    "apikey-gen": "sh -c 'API_KEY=$(docker compose exec aggregator /ava create-api-key --role=admin --subject=apikey) && echo \"Generated API key for tests: $API_KEY\" && echo \"Writing to .env.test to replace the TEST_API_KEY env variable\" && sed -i \"\" -e \"s/^TEST_API_KEY=.*/TEST_API_KEY=$API_KEY/\" .env.test && echo \"Done.\"'",
    "build": "yarn workspaces run build",
    "clean": "rm -rf node_modules yarn.lock && yarn workspaces run clean",
    "test": "jest --config jest.config.cjs --detectOpenHandles",
    "test:auth": "dotenv -e .env.$TEST_ENV jest tests/auth.test.ts --verbose --runInBand",
    "test:createWorkflow": "dotenv -e .env.$TEST_ENV jest tests/createWorkflow.test.ts --verbose --runInBand",
    "test:getWorkflow": "dotenv -e .env.$TEST_ENV jest tests/getWorkflow.test.ts --verbose --runInBand",
    "test:getWorkflows": "dotenv -e .env.$TEST_ENV jest tests/getWorkflows.test.ts --verbose --runInBand",
    "test:cancelWorkflow": "dotenv -e .env.$TEST_ENV jest tests/cancelWorkflow.test.ts --verbose --runInBand",
    "test:deleteWorkflow": "dotenv -e .env.$TEST_ENV jest tests/deleteWorkflow.test.ts --verbose --runInBand",
    "test:triggerWorkflow": "dotenv -e .env.$TEST_ENV jest tests/triggerWorkflow.test.ts --verbose --runInBand",
    "test:getExecution": "dotenv -e .env.$TEST_ENV jest tests/getExecution.test.ts --verbose --runInBand",
    "test:getExecutions": "dotenv -e .env.$TEST_ENV jest tests/getExecutions.test.ts --verbose --runInBand",
    "test:getWallet": "dotenv -e .env.$TEST_ENV jest tests/getWallet.test.ts --verbose --runInBand",
    "test:getWallets": "dotenv -e .env.$TEST_ENV jest tests/getWallets.test.ts --verbose --runInBand",
    "test:secret": "dotenv -e .env.$TEST_ENV jest tests/secret.test.ts --verbose --runInBand",
    "test:node": "dotenv -e .env.$TEST_ENV jest tests/runNodeWithInputs.test.ts --verbose --runInBand",
    "test:loop": "dotenv -e .env.$TEST_ENV jest tests/loopNode.test.ts --verbose --runInBand",
    "test:customcode": "dotenv -e .env.$TEST_ENV jest tests/customCode.test.ts --verbose --runInBand",
    "test:event": "dotenv -e .env.$TEST_ENV jest tests/eventTrigger.test.ts --verbose --runInBand",
    "test:select": "npm run test -- --testNamePattern",
    "test:docker": "bash ./scripts/run-tests-with-docker.sh",
    "build:example": "tsup examples/example.ts --format cjs,esm --watch examples/"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@changesets/cli": "^2.27.9",
    "@eslint/js": "^9.16.0",
    "@jest/globals": "^29.7.0",
    "@types/axios": "^0.14.4",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "axios": "^1.8.4",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "envalid": "^8.0.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "grpc-tools": "^1.13.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  }
}
