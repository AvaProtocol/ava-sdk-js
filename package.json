{
  "name": "@avaprotocol/sdk-js",
  "version": "0.6.9",
  "description": "A JavaScript/TypeScript SDK designed to simplify integration with Ava Protocolâ€™s AVS",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "author": "Vinh Nguyen (https://github.com/v9n), Chris Li (https://github.com/chrisli30)",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "gen-protoc": "curl -s https://raw.githubusercontent.com/AvaProtocol/EigenLayer-AVS/main/protobuf/avs.proto > grpc_codegen/avs.proto && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./grpc_codegen/ --grpc_out=grpc_js:./grpc_codegen/ --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./grpc_codegen --proto_path=./grpc_codegen grpc_codegen/avs.proto",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs --testMatch '**/src/__tests__/*.e2e.test.ts'"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.3",
    "@grpc/proto-loader": "^0.7.13",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.2",
    "google-protobuf": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "type": "module"
}
