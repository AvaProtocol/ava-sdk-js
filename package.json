{
  "name": "ava-sdk-js",
  "version": "0.6.0",
  "description": "A JavaScript/TypeScript SDK designed to simplify integration with Ava Protocolâ€™s AVS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "author": "Vinh Nguyen (https://github.com/v9n), Chris Li (https://github.com/chrisli30)",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "gen-protoc": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./grpc_codegen/ --grpc_out=grpc_js:./grpc_codegen/ --proto_path=../../submodules/EigenLayer-AVS/protobuf avs.proto",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.3",
    "@grpc/proto-loader": "^0.7.13",
    "ethers": "^6.13.2",
    "google-protobuf": "^3.21.4"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "type": "module"
}
