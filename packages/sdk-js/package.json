{
  "name": "@avaprotocol/sdk-js",
  "version": "2.6.4",
  "description": "A JavaScript/TypeScript SDK designed to simplify integration with Ava Protocol's AVS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "author": "Vinh Nguyen (https://github.com/v9n), Chris Li (https://github.com/chrisli30)",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.18.0",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "build:declarations": "tsc -b tsconfig.json",
    "build:js": "tsup src/index.ts --format cjs,esm",
    "build": "yarn build:declarations && yarn build:js",
    "clean": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "prepare": "node ../../scripts/prepare-package.js"
  },
  "dependencies": {
    "@avaprotocol/types": "^2.4.4",
    "@grpc/grpc-js": "^1.11.3",
    "@grpc/proto-loader": "^0.7.13",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.2",
    "google-protobuf": "3.19.4",
    "id128": "^1.6.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12"
  }
}
