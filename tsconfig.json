{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "outDir": "./dist",
    "declaration": true,
    "rootDir": "src"
  },
  "include": [
    "src/**/*",
    "grpc_codegen/**/*"
  ],
  "ts-node": {
    "esm": true
  }
}
